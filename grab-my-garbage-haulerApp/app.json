{
  "expo": {
    "name": "grab-my-garbage-driver",
    "slug": "grab-my-garbage-driver",
    "version": "7.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/icon.png",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ebf4f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rilah.grabmygarbagedriver",
      "config": {
        "googleMapsApiKey": "AIzaSyAbfkVeoFpJ3IFgmb48FSWuKFWQsLYteKA"
      }
    },
    "android": {
      "package": "com.rilah.grabmygarbagedriver",
      "versionCode": 7,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAbfkVeoFpJ3IFgmb48FSWuKFWQsLYteKA"
        }
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/app/icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/app/icon.png"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos."
        }
      ]
    ]
  }
}
